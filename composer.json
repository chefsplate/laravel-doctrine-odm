{
  "name": "chefsplate/laravel-doctrine-odm",
  "description": "A smart, lightweight Laravel wrapper around Doctrine's mongodb-odm Document Mapper",
  "keywords": ["laravel", "doctrine", "mongo", "mongodb", "document", "mapper", "odm"],
  "homepage": "https://github.com/chefsplate/laravel-doctrine-odm",
  "license": "MIT",
  "authors": [
    {
      "name": "David Chang",
      "email": "davidchchang@gmail.com"
    },
    {
      "name": "Brent Manza",
      "email": "brent.manza@chefsplate.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "5.1.*|5.2.*",
    "doctrine/mongodb-odm": "~1.0",
    "doctrine/mongodb-odm-softdelete": "dev-master",
    "phpdocumentor/reflection-docblock": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "ChefsPlate\\ODM\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
