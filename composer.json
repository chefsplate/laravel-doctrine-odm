{
  "name": "chefsplate/laravel-doctrine-odm",
  "description": "A smart, lightweight Laravel wrapper around Doctrine's mongodb-odm Document Mapper",
  "keywords": ["laravel", "doctrine", "mongo", "mongodb", "document", "mapper", "odm"],
  "homepage": "https://github.com/chefsplate/laravel-doctrine-odm",
  "license": "MIT",
  "authors": [
    {
      "name": "David Chang",
      "email": "davidchchang@gmail.com"
    },
    {
      "name": "Brent Manza",
      "email": "brent.manza@chefsplate.com"
    }
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "chefsplate/mongodb-odm-softdelete",
        "version": "0.1.1",
        "source": {
          "url": "https://github.com/chefsplate/mongodb-odm-softdelete.git",
          "type": "git",
          "reference": "origin/master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "5.3.*|5.4.*",
    "ext-mongo": "*",
    "doctrine/mongodb-odm": "^1.1",
    "mongodb/mongodb": "^1.1",
    "chefsplate/mongodb-odm-softdelete": "^0.1",
    "phpdocumentor/reflection-docblock": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "ChefsPlate\\ODM\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
